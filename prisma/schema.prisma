  airline_en  String?   @db.VarChar(255)
  icao_code   String?   @db.VarChar(255)
  iata_code   String?   @db.VarChar(255)
  remark      String?   @db.VarChar(255)
  status      String?   @db.VarChar(255)
  create_user Int?
